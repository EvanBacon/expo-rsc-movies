{
  "expo": {
    "name": "Expo Movies",
    "slug": "rsc-movies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "x.movie",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "web"],
    "web": {
      "output": "server",
      "favicon": "./assets/images/icon.png"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "app.expo.flix"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://flix.expo.app/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "reactServerFunctions": true,
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "07793961-ad0c-43ca-bce1-301475ecd214"
      }
    }
  }
}
